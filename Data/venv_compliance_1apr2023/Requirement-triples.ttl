ex:154709a8cf0848ae8c16a19844f9424b a comp:RequirementSection;
  comp:name "AMSE B30.9-2018, Section: 9-5.9.5";
  comp:sectionNumber "9-5.9.5";
  comp:description """Web sling should be removed from service and be discarded if the following conditions are found:

