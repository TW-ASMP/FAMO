#################################################################
#    Rules
#################################################################

:rec1 rdf:type swrl:Variable .

:p rdf:type swrl:Variable .

:rec2 rdf:type swrl:Variable .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Rule to infer whether a record currently represents the child of another (analogous to subrole of inference)" ;
   rdfs:label "represents child role of record" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :representsChildRoleOfRecord ;
                           swrl:argument1 :rec1 ;
                           swrl:argument2 :rec2
                         ] ;
               rdf:rest rdf:nil
             ] ;
   swrl:head rdf:nil
 ] .

[ swrla:isRuleEnabled "true"^^xsd:boolean ;
   rdfs:comment "Rule to infer that a record currently represents the child (sub role) of a role represented by another record" ;
   rdfs:label "represents child of role record" ;
   rdf:type swrl:Imp ;
   swrl:body [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate <http://ontology.eil.utoronto.ca/FAMO/famo/representsSubjectOf> ;
                           swrl:argument1 :rec1 ;
                           swrl:argument2 :p
                         ] ;
               rdf:rest [ rdf:type swrl:AtomList ;
                          rdf:first [ rdf:type swrl:ClassAtom ;
                                      swrl:classPredicate <http://ontology.eil.utoronto.ca/FAMO/famo/CurrentPropertyManifestation> ;
                                      swrl:argument1 :p
                                    ] ;
                          rdf:rest [ rdf:type swrl:AtomList ;
                                     rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                                                 swrl:propertyPredicate <http://ontology.eil.utoronto.ca/FAMO/famo/representsObjectOf> ;
                                                 swrl:argument1 :rec2 ;
                                                 swrl:argument2 :p
                                               ] ;
                                     rdf:rest rdf:nil
                                   ]
                        ]
             ] ;
   swrl:head [ rdf:type swrl:AtomList ;
               rdf:first [ rdf:type swrl:IndividualPropertyAtom ;
                           swrl:propertyPredicate :representsChildRoleOfRecord ;
                           swrl:argument1 :rec1 ;
                           swrl:argument2 :rec2
                         ] ;
               rdf:rest rdf:nil
             ]
 ] .

